<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="WebAppLibreria"
                   namespace="WebAppLibreria.Dominio"
                   default-lazy="false">
  <class name="Cliente" table="Cliente">
    <id name="Id" access="property" column="ClienteID">
      <generator class="native" />
    </id>
    <property name="Nombre" column="Nombre" />
    <property name="ApePater" column="ApellidoPaterno" />
    <property name="ApeMater" column="ApellidoMaterno" />
    <property name="Sueldo" column="Sueldo" />
    <property name="Direccion" column="Direccion" />
    <property name="Telefono" column="Telefono" />
    <property name="Usuario" column="Usuario" />
    <property name="Clave" column="Clave" />
    <many-to-one name="TipoDocumento" column="TipoDocumentoID"/>
    <many-to-one name="Distrito" column="DistritoID"/>
  </class>
</hibernate-mapping>
