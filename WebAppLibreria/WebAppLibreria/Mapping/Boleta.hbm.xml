<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="WebAppLibreria"
                   namespace="WebAppLibreria.Dominio"
                   default-lazy="false">
  <class name="Boleta" table="Boleta">
    <id name="Id" access="property" column="BoletaID">
      <generator class="native" />
    </id>
    <many-to-one name="Cliente" column="ClienteID"/>
    <many-to-one name="Empleado" column="EmpleadoID"/>    
    <property name="FechaPedido" column="FechaPedido" />
    <property name="Observacion" column="Observacion" />
  </class>
</hibernate-mapping>
