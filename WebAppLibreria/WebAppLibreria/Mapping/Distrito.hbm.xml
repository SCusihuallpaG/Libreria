<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="WebAppLibreria"
                   namespace="WebAppLibreria.Dominio"
                   default-lazy="false">
  <class name="Distrito" table="Distrito">
    <id name="Id" access="property" column="DistritoID">
      <generator class="native" />
    </id>
    <property name="Nombre" column="NombreDist" />
  </class>
</hibernate-mapping>