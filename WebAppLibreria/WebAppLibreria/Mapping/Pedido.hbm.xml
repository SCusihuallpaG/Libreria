<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="WebAppLibreria"
                   namespace="WebAppLibreria.Dominio"
                   default-lazy="false">
  <class name="Pedido" table="Pedido">
    <id name="Id" access="property" column="PedidoID">
      <generator class="native" />
    </id>
    <property name="FechaPedido" column="FechaPedido"/>
    <property name="Observacion" column="Observacion" />
    <many-to-one name="Cliente" column="ClienteID"/>
  </class>
</hibernate-mapping>
